    "@types/react-dom": "^17.0.5",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.8.3",
    "cssnano": "^5.0.4",
    "husky": "^6.0.0",
    "postcss": "^8.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "regenerator-runtime": "^0.13.7",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^0.0.15",
